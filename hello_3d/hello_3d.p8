function vec3_init(x,y,z)
	tmp = {x,y,z}
	return tmp
end

function vec3_length(v)
return sqrt(
	v[1] * v[1]+
	v[2] * v[2]+
	v[3] * v[3]
	)
end

function vec3_scale(v,s)
return {
	v[1] * s, 
	v[2] * s, 
	v[3] * s
	}
end

function vec3_normalize(v)
return vec3_scale(v, 1 / vec3_length(v))
end

function vec3_add(a, b)
return { 
	a[1] + b[1], 
	a[2] + b[2], 
	a[3] + b[3] 
	}
end

function vec3_sub(a,b)
return { 
	a[1] - b[1], 
	a[2] - b[2], 
	a[3] - b[3] 
	} 
end

function vec3_dot(a,b)
return {
	a[1] * b[1]+
	a[2] * b[2]+
	a[3] * b[3]
	}
end

function vec3_cross(a, b)
  return { 
	a[2] * b[3] - a[3] * b[2], 
	a[3] * b[1] - a[1] * b[3], 
	a[1] * b[2] - a[2] * b[1] 
	}
end
